{
    "name": "zephir/kirby-contentsync",
    "description": "Kirby contentsync plugin",
    "license": "MIT",
    "type": "kirby-plugin",
    "version": "2.1.0",
    "authors": [
        {
            "name": "Marc Stampfli",
            "email": "marc@zephir.ch"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.2",
        "getkirby/cli": "^1.1",
        "php-curl-class/php-curl-class": "^9.19"
    },
    "require-dev": {
        "getkirby/cms": "4.0-beta.2"
    },
    "autoload": {
        "psr-4": {
            "Zephir\\Contentsync\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
